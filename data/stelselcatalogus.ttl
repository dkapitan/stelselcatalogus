  sc:herkomst "Inschrijvingen betreffende registergoederen."@nl-nl ;
  sc:kwaliteit "Zie: kwaliteitshandvest op www.kadaster.nl/kwaliteitshandvest"@nl-nl ;
  sc:referentieWetgeving <http://opendata.stelselcatalogus.nl/stelselcatalogus/id/regelgeving/kadasterwet/art_48> ;
